	keywords = {Questionnaires},
	file = {EBSCO Full Text:C\:\\Users\\Nami Sunami\\Zotero\\storage\\HEEFG22S\\Downey and Feldman - 1996 - Implications of rejection sensitivity for intimate.pdf:application/pdf;APA Psycnet Fulltext PDF:C\:\\Users\\Nami Sunami\\Zotero\\storage\\BAVPGQSI\\Downey and Feldman - 1996 - Implications of rejection sensitivity for intimate.pdf:application/pdf;APA Psycnet Fulltext PDF:C\:\\Users\\Nami Sunami\\Zotero\\storage\\TBUQ9CE7\\Downey and Feldman - 1996 - Implications of rejection sensitivity for intimate.pdf:application/pdf},
