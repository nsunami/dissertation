@article{downey_self-fulfilling_1998,
	title = {The self-fulfilling prophecy in close relationships: Rejection sensitivity and rejection by romantic partners},
	volume = {75},
	rights = {(c) 2016 {APA}, all rights reserved},
	issn = {1939-1315 0022-3514},
	doi = {10.1037/0022-3514.75.2.545},
	shorttitle = {The self-fulfilling prophecy in close relationships},
	abstract = {The authors hypothesized a self-fulfilling prophecy wherein rejection expectancies lead people to behave in ways that elicit rejection from their dating partners. The hypothesis was tested in 2 studies of conflict in couples: (a) a longitudinal field study where couples provided daily-diary reports and (b) a lab study involving behavioral observations. Results from the field study showed that high rejection-sensitive ({HRS}) people's relationships were more likely to break up than those of low rejection-sensitive ({LRS}) people. Conflict processes that contribute to relationship erosion were revealed for {HRS} women but not for {HRS} men. Following naturally occurring relationship conflicts, {HRS} women's partners were more rejecting than were {LRS} women's partners. The lab study showed that {HRS} women's negative behavior during conflictual discussions helped explain their partners' more rejecting postconflict responses.},
	pages = {545--560},
	number = {2},
	journaltitle = {Journal of Personality and Social Psychology},
	author = {Downey, Geraldine and Freitas, Antonio L. and Michaelis, Benjamin and Khouri, Hala},
	date = {1998},
	note = {tex.ids: downey1998a},
	file = {APA Psycnet Fulltext PDF:C\:\\Users\\Nami Sunami\\Zotero\\storage\\SSNQJF9Q\\Downey et al. - 1998 - The self-fulfilling prophecy in close relationship.pdf:application/pdf},
}

@article{mattick_development_1998,
