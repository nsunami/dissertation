	note = {tex.ids: leary1995a},
	file = {EBSCO Full Text:C\:\\Users\\Nami Sunami\\Zotero\\storage\\6AAK5EQ4\\Leary et al. - 1995 - Self-esteem as an interpersonal monitor The socio.pdf:application/pdf},
}

@article{downey_implications_1996,
